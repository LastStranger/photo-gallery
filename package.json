{
  "name": "photo-gallery",
  "version": "1.0.0",
  "description": "Photo Gallery",
  "main": "src/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "format": "prettier --write --single-quote --print-width=120 --parser=flow index.js webpack.config.js \"src/**/*.{js,jsx,css}\"",
    "lint": "eslint -c ./.eslintrc.json src/**/*.{js,jsx} webpack.config.js",
    "size-css": "node -e \"process.stdout.write('CSS gzip size: ')\" && gzip-size public/assets/styles.css",
    "size-js": "node -e \"process.stdout.write('JS gzip size: ')\" && gzip-size public/assets/bundle.js",
    "test": "jest",
    "test:update": "jest -u",
    "types": "flow",
    "validate": "run-s -s -l format lint types test size-css size-js"
  },
  "jest": {
    "transform": {".*": "<rootDir>/node_modules/jest-css-modules"},
    "snapshotSerializers": [
      "jest-serializer-html-string"
    ]
  },
  "keywords": [],
  "author": "Brian Holt <btholt@gmail.com>",
  "license": "MIT",
  "eslintConfig": {},
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.0.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.47.0",
    "flowtype-loader": "^0.2.2",
    "gzip-size-cli": "^2.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-serializer-html-string": "^1.0.1",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "preact-render-to-string": "^3.6.3",
    "prettier": "^1.4.0",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "flickrapi": "^0.6.0",
    "preact": "^8.1.0",
    "preact-router": "^2.5.2"
  }
}
